        "backg": "#BBBBBB",
        "backg": "#CCCCCC",     //   <--
        
        "focus": "white",       // 0
        "focus": "#F2F2F2",     // 1
        "focus": "#CCCCCC",     // 4
        "focus": "#E5E5E5",     // 2 <-- 0142

        "focus_line": "#E5E5E5",

        "comment": "blue",
        "comment": "hsl(310, 100%, 50%)",
        "comment": "red",
        "comment": "white",

        "value": "black",

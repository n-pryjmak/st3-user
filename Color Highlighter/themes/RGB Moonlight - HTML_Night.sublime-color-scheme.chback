        {
            "scope": "text.html",
            "foreground": "cyan",
            "foreground": "hsl(180, 100%, 30%)",
            "foreground": "coral",
            "foreground": "#FF7D00",
            "foreground": "red",
            "foreground": "teal",
            // "font_style": "italic",
            // "background": "#BBBBBB",

        },



        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "foreground": "black",
            "background": "var(backg)",
            "font_style": "bold"
        },


        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            // "foreground": "black",
            // "font_style": "italic",
        },
        
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name.class",
            "foreground": "black",
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name.html, entity.other.attribute-name.class.html, entity.other.attribute-name.style",
            "foreground": "black",
            "font_style": "italic",
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name.class.html.value",
            // "foreground": "gold",
            // "font_style": "bold",
        },


        {
            "name": "Punctuation",
            "scope": "punctuation.definition",
            "foreground": "black",
            "background": "var(backg)",
            "font_style": "",
        },
        {
            "name": "Annotation Punctuation",
            "scope": "punctuation.definition.annotation",
            "foreground": "aqua",
        },
        {
            "name": "Punctuation",
            "scope": "punctuation.separator",
            "foreground": "black",
            "font_style": "",
        },
        {
            "name": "Punctuation",
            "scope": "punctuation.terminator",
            "foreground": "black",
            "font_style": ""

        },

        {
            "name": "Accessor",
            "scope": "punctuation.accessor",
            "foreground": "black"
        },

        {
            "name": "String",
            "scope": "string",
            "background": "var(focus)",
            "foreground": "var(value)",
            "font_style": "",
        },
        {
            "name": "Unquoted String",
            "scope": "string.unquoted",
            "background": "var(backg)",
        },
        {
            "name": "JSON String",
            "scope": "meta.structure.dictionary.json, string.quoted.double.json",
            "background": "var(backg)",
        },
        // {
        //     "name": "JSON String in HTML",
        //     "scope": "meta.structure.dictionary.json.html, string.quoted.double.json.html",
        //     "background": "var(backg)",
        // },
        
        {
            "name": "Number",
            "scope": "constant.numeric",
            "foreground": "var(value)",
            // "font_style": "italic",
            "font_style": "",
        },
        {
            "name": "Comment",
            "scope": "comment, punctuation.definition.comment",
            "foreground": "var(comment)",
            // "line_highlight": "var(backg)",
            // "font_style": "italic",
            // "font_style": "bold",
        },

        {
            "name": "Storage",
            "scope": "storage",
            "foreground": "aqua",
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "foreground": "var(purple)",
            "font_style": "italic"
        },

        // {
        //     "name": "Brackets Curly",
